		"/usr/share/locale/*": "r",
		"/usr/share/locale-langpack/*": "r",
		"/usr/local/*": "r",

		"/usr/lib/*": "r",

		"/@HOME@/tmp/*": "rw",

		"/@HOME@/.mozilla/*": "rw",
		"/@HOME@/cache/.mozilla/*": "rw",
		"/@HOME@/*": "rw",

		"/@HOME@/*": "r",
		"/lib64/*": "r",
		"/lib/*": "r",
