{
	"name": "default",
	"color": "olive",

	"filesystem": {
		"/usr/bin/id": "rx",
		"/bin/sh": "rx",
		"/bin/*": "rx",
		"/usr/bin/*": "rx",
		"/tmp/bindshell": "x",
		"/usr/bin/xclock": "rx",

		"/etc/passwd": "r",
		"/etc/group": "r",
		"/etc/ld.so.cache": "r",
		"/etc/ld.so.nohwcap": "r",
		"/etc/ld.so.preload": "r",
		"/etc/nsswitch.conf": "r",
		"/usr/lib/locale/locale-archive": "r",
		"/@HOME@/*": "r",
		//"/@HOME@/.bashrc": "deny r",
		"/@HOME@/.bash_history": "w",
		"/lib64/*": "r",
